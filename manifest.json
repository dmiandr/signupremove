{
  "manifest_version": 2,
  "name": "SignUp Propo Remover",
  "short_name": "SignUpRemove",
  "version": "0.1",
  "description": "Removes annoying SignUp popup on VK, FB and Instagram",

  "developer": {
        "name": "dmitrevo",
        "url": "https://cont.ws/@dmitrevo"
    },

  "homepage_url": "https://github.com/dmiandr",

  "icons": {
    "48": "nofb48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://vk.com/*", "*://*.instagram.com/*", "*://*.facebook.com/*"],
      "js": ["content.js"]
    }
  ]
}
